      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- Load KPK"
      DL.SendSequence "Load Plaintext Key - command 1 (80-00)"
      result = DL.WaitForSequence("80-xx ok resp", 1, 2000)
