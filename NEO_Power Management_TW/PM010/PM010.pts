   ledcheck = msgbox("Reader is RUN mode?", vbyesno)
   if ledcheck = 6 then
      result = 1
   else
      result = 0
   end if
end if
'------------------------------------------------------------
If result <> 0 Then
   DL.ResetReceiveCounter
