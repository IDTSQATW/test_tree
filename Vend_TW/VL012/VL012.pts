if result <> 0 then
   dl.resetreceivecounter
   dl.addcomment "--- 05-01"
   DL.SendSequence "05-01"
   result = DL.WaitForSequence("05 00", 1, 5000)
   dl.pause 1000
end if

if result <> 0 then
   dl.resetreceivecounter
   DL.AddComment vbCrLf
   dl.addcomment "--- Disable Blue LEDs"
   DL.SendSequence "Disable Blue LEDs"
   result = DL.WaitForSequence("F0 00", 1, 5000)
   dl.pause 1000
end if
'------------------------------------------------------------Enc OFF, swipe card
   if result <> 0 then
      dl.resetreceivecounter
      DL.AddComment vbCrLf
      dl.addcomment "--- C7-36 = 00"
      DL.SendSequence "C7-36 = 00"
      result = DL.WaitForSequence("C7 00", 1, 5000)
