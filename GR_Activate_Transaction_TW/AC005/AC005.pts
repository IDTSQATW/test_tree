      if r2 = 0 then dl.addcomment vbcrlf & "Tag 9F39: FAIL!"
      if r3 = 0 then dl.addcomment vbcrlf & "Tag DF30: FAIL!"
      if r4 = 0 then dl.addcomment vbcrlf & "Tag DFEE26: FAIL!"
      if r5 = 0 then dl.addcomment vbcrlf & "Tag 5A: FAIL!"
      if r6 = 0 then dl.addcomment vbcrlf & "Tag 57: FAIL!"
      if r7 = 0 then dl.addcomment vbcrlf & "Tag 9F02: FAIL!"
   end if
End If
