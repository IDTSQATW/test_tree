              DL.SendSequence "04-00 w/ tag FFEE06 (VAS OR Pay)"
              result = DL.WaitForSequence("04 00", 1, 2000)
              dl.pause 500
           end if
        end if

        if j = 3 then 
           if result <> 0 then
              dl.resetreceivecounter
              dl.addcomment vbcrlf
              DL.AddComment "--- 04-00 w/ tag FFEE06 (VAS AND Pay)"
              DL.SendSequence "04-00 w/ tag FFEE06 (VAS AND Pay)"
              result = DL.WaitForSequence("04 00", 1, 2000)
              dl.pause 500
           end if
        end if

        if j = 4 then 
           if result <> 0 then
              dl.resetreceivecounter
              dl.addcomment vbcrlf
              DL.AddComment "--- 04-00 w/ tag FFEE06 (PAY only)"
              DL.SendSequence "04-00 w/ tag FFEE06 (PAY only)"
              result = DL.WaitForSequence("04 00", 1, 2000)
              dl.pause 500
           end if
        end if

        if j <= 3 then     'can get VAS data
           if result <> 0 then
              dl.resetreceivecounter
              dl.addcomment vbcrlf
              DL.AddComment "--- VAS data auto output"
              msgbox "Tapped iPhone (Panera card) --> Click OK", vbokonly
              if i = 1 then 
                 dl.resetreceivecounter
                 dl.addcomment vbcrlf
                 DL.AddComment "--- 03-40"
                 DL.SendSequence "03-40"
                 result = DL.WaitForSequence("03 57 - VAS data", 1, 2000)
              end if
              if i = 2 or i = 3 then result = DL.WaitForSequence("02 57 - VAS data", 1, 2000)
              if result <> 0 then result = DL.WaitForSequence("other tags", 1, 2000)
              dl.pause 500
           end if
        else               'can NOT get VAS data  
           if result <> 0 then
              dl.resetreceivecounter
              dl.addcomment vbcrlf
              DL.AddComment "--- VAS data auto output"
              msgbox "Tapped iPhone (Panera card) --> Click OK", vbokonly
              if i = 1 then 
                 dl.resetreceivecounter
                 dl.addcomment vbcrlf
                 DL.AddComment "--- 03-40"
                 DL.SendSequence "03-40"
                 result = DL.WaitForSequence("03 00 - VAS fail", 1, 2000)
              end if
              if i = 2 then 
                 result = DL.WaitForSequence("0A", 1, 2000)
                 ReverseResult(result)
              end if
              if i = 3 then result = DL.WaitForSequence("02 0A - VAS fail", 1, 2000)
              dl.pause 500
           end if        
        end if
    next
next

DL.AddComment vbCrLf
