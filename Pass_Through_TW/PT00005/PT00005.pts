   DL.SendSequence "3colorLED off"
   result = DL.WaitForSequence("LED command Status (OK)", 1, 10000)
End If
DL.Pause 1500
'------------------------------------------------------------
If result <> 0 Then
   DL.ResetReceiveCounter
   DL.AddComment vbCrLf & "--- Pass-Through - STOP"
   DL.SendSequence "Pass-Through - STOP"
   result = DL.WaitForSequence("Pass-Through - START and STOP (OK)", 1, 10000)

   usercheck = msgbox("Does LEDs work properly?", vbyesno)
   if usercheck = 6 then
