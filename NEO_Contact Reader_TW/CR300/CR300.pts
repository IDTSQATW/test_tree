sub CTNormalTran()
If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 60-10 Contact Start Transaction *** " & readerType 
   DL.SendSequence ctcmd 
   result = DL.WaitForSequence("60-xx 63 status", 1, 10000)
   if result <> 0 then
      msgbox "Click OK, insert card (EMV Test Card, T=0)", vbokonly

      if readerType = "GR" or readerType = "NEO1" then 
         r1 = DL.WaitForSequence("60-10 00 10", 1, 10000)
      elseif readerType = "NEO2" or readerType = "USAT" then 
         r1 = DL.WaitForSequence("60-10 00 10 (NEO2)", 1, 10000)
      else
         r1 = DL.WaitForSequence("60-10 00 10", 1, 10000)
      end if

      if readerType = "GR" or readerType = "NEO1" then 
         r2 = DL.WaitForSequence("dfee26", 1, 2000)
      elseif readerType = "NEO2" or readerType = "USAT" then 
         r2 = DL.WaitForSequence("dfee26 (NEO2)", 1, 2000)
      else
         r2 = DL.WaitForSequence("dfee26", 1, 2000)
      end if

      if readerType = "GR" or readerType = "NEO1" then 
         r3 = DL.WaitForSequence("dfef4c-4d", 1, 2000)
      elseif readerType = "NEO2" or readerType = "USAT" then 
         r3 = 1
      else
         r3 = DL.WaitForSequence("dfef4c-4d", 1, 2000)
      end if

      if r1 = 0 or r2 = 0 or r3 = 0 then result = 0
   end if
end if

If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 60-11 Contact Authenticate Transaction"
   DL.SendSequence "60-11"
   result = DL.WaitForSequence("60-xx 63 status", 1, 10000)
   if result <> 0 then

      if readerType = "GR" or readerType = "NEO1" then 
         result = DL.WaitForSequence("60-11 00 04", 1, 10000)
      elseif readerType = "NEO2" or readerType = "USAT" then 
         result = DL.WaitForSequence("60-11 00 04 (NEO2)", 1, 10000)
      else
         result = DL.WaitForSequence("60-11 00 04", 1, 10000)
      end if

   end if
end if

If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 60-12 Contact Apply Host Response"
   DL.SendSequence "60-12"
   result = DL.WaitForSequence("60-xx 63 status", 1, 10000)
   if result <> 0 then

      if readerType = "GR" or readerType = "NEO1" then 
         result = DL.WaitForSequence("60-12 00 02", 1, 10000)
      elseif readerType = "NEO2" or readerType = "USAT" then 
         result = DL.WaitForSequence("60-12 00 02 (NEO2)", 1, 10000)
      else
         result = DL.WaitForSequence("60-12 00 02", 1, 10000)
      end if

   end if
end if

if result <> 0 then
   behaviorcheck = msgbox ("Is reader behavior correct (LCD/ Buzzer)?", vbyesno)
   dl.pause 1000
   if behaviorcheck = 7 then result = 0
else
   dl.sendsequence "05-01 Cancel transaction"
   dl.pause 1000
end if 
end sub  

sub CTFastEMVTran()
If result <> 0 Then
   msgbox "Please notice LCD/ Buzzer, correct processes should be: (LCD) please wait/ processing/ approved (4 sec) -> (LCD) remove card + (Buzzer) persist to beep -> (User remove the card) -> (LCD) welcome", vbokonly 
   dl.addcomment vbcrlf & vbcrlf & "Please notice LCD/ Buzzer, correct processes should be: (LCD) please wait/ processing/ approved (4 sec) -> (LCD) remove card + (Buzzer) persist to beep -> (User remove the card) -> (LCD) welcome"
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 60-10 Contact Start Transaction"
   DL.SendSequence ctcmd 
   result = DL.WaitForSequence("60-xx 63 status", 1, 10000)
   if result <> 0 then
      msgbox "Click OK, insert card (EMV Test Card, T=0)", vbokonly

      if readerType = "GR" or readerType = "NEO1" then 
         r1 = DL.WaitForSequence("60-10 00 10", 1, 10000)
      elseif readerType = "NEO2" or readerType = "USAT" then 
         r1 = DL.WaitForSequence("60-12 00 03 FastEMV (NEO2)", 1, 10000)
      else
         r1 = DL.WaitForSequence("60-10 00 10", 1, 10000)
      end if

      if readerType = "GR" or readerType = "NEO1" then 
         r2 = DL.WaitForSequence("dfee26", 1, 2000)
      elseif readerType = "NEO2" or readerType = "USAT" then 
         r2 = DL.WaitForSequence("dfee26 (NEO2)", 1, 2000)
      else
         r2 = DL.WaitForSequence("dfee26", 1, 2000)
      end if

      if readerType = "GR" or readerType = "NEO1" then 
         r3 = DL.WaitForSequence("dfef4c-4d", 1, 2000)
      elseif readerType = "NEO2" or readerType = "USAT" then 
         r3 = 1
      else
         r3 = DL.WaitForSequence("dfef4c-4d", 1, 2000)
      end if

      if r1 = 0 or r2 = 0 or r3 = 0 then result = 0
   end if
end if

if result <> 0 then
   behaviorcheck = msgbox ("Is reader behavior correct (LCD/ Buzzer)?", vbyesno)
   dl.pause 1000
   if behaviorcheck = 7 then result = 0
else
   dl.sendsequence "05-01 Cancel transaction"
   dl.pause 1000
end if 
end sub  

else
   DL.AddComment "The reader did not support FastEMV, please skip this case."
end if
