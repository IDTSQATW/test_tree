sub at()
   if result <> 0 then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      if poll = 1 then 
         MsgBox "Click OK first, tap VISA qVSDC", VBOKOnly
         DL.AddComment "--- 02-40 Activate Transaction"
         DL.SendSequence "02-40"
         result = DL.WaitForSequence("02-40 resp", 1, 6000)
      elseif poll = 0 then
             MsgBox "Tap VISA qVSDC first, click OK", VBOKOnly
             DL.AddComment "--- 03-40 Activate Transaction"
             DL.SendSequence "03-40"
             result = DL.WaitForSequence("03-40 resp", 1, 6000)
      end if

   if result <> 0 then
      r(1) = dl.waitforsequence("DFEF4C " & i, 1, 1000)
           if r(1) = 0 then dl.addcomment vbcrlf & "DFEF4C...FAIL!"
      r(2) = dl.waitforsequence("DFEF4D " & i, 1, 1000)
           if r(2) = 0 then dl.addcomment vbcrlf & "DFEF4D...FAIL!"

      countfail()
   '------------------------------------------------------------
   else 
      countf = countf + 1
      dl.addcomment vbcrlf & "** RX format...FAIL! **"
   end if
   else 
      countf = countf + 1
      dl.addcomment vbcrlf & "** Poll Response...FAIL! **"
   End If
   dl.pause 500
end sub
