'Debugging (!!!!! DO NOT EDIT !!!!!)
'------------------------------------------------------------
Function MyDebug(DebugString)
      DL.AddComment  vbCrLf & DebugString & " = " & result & vbCrLf
      MsgBox DebugString & " = " & result, VBOKOnly, "Debug"
End Function
'------------------------------------------------------------
'TEST BEGINS
'------------------------------------------------------------
If result <> 0 Then
   dl.resetreceivecounter
   DL.AddComment "--- 04-09"
   DL.SendSequence "04-09"
   result = DL.WaitForSequence("04-09", 1, 10000)
   dl.pause 1000
End If

if readerType = "NEO2" then
   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- First response control = Send First Response 0x63"
      DL.SendSequence "First response control = Send First Response 0x63"
      result = DL.WaitForSequence("04 00", 1, 5000)
   End If

   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 60-03 Contact Set Application Data (VISA)"
      DL.SendSequence "60-03 Contact Set Application Data (VISA)"
      result = DL.WaitForSequence("60-xx 00 status", 1, 3000)
      dl.pause 1000
   End If

   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 60-0A Contact Set CA Public Key"
      DL.SendSequence "60-0A Contact Set CA Public Key"
      result = DL.WaitForSequence("60-xx 00 status", 1, 3000)
      dl.pause 1000
   End If

   lcdcheck = msgbox ("Does the reader have LCD?", vbyesno)

   if lcdcheck = 6 then
   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 60-16 4C_Contact Set ICS Identification"
      DL.SendSequence "60-16 Contact Set ICS Identification (04)"
      result = DL.WaitForSequence("60-xx 00 status", 1, 3000)
      dl.pause 1000
   End If

   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 60-06 4C_Contact Set Terminal Data"
      DL.SendSequence "60-06 4C config (NEO2)"
      result = DL.WaitForSequence("60-xx 00 status", 1, 3000)
      dl.pause 1000
   End If
   elseif lcdcheck = 7 then
   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 60-16 2C_Contact Set ICS Identification"
      DL.SendSequence "60-16 2C_Contact Set ICS Identification"
      result = DL.WaitForSequence("60-xx 00 status", 1, 3000)
      dl.pause 1000
   End If

   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 60-06 2C_Contact Set Terminal Data (9f39=05)"
      DL.SendSequence "60-06 2C_Contact Set Terminal Data (9f39=05)"
      result = DL.WaitForSequence("60-xx 00 status", 1, 3000)
      dl.pause 1000
   End If
   end if
end if

If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 60-10 Contact Start Transaction *** " & readerType 
   DL.SendSequence "60-10 fallback"
   result = DL.WaitForSequence("60-xx 63 status", 1, 10000)
   if result <> 0 then
      msgbox "Click OK --> Insert card (EMV Test Card, T=0)", vbokonly
      if readerType <> "NEO2" then 
         r1 = DL.WaitForSequence("60-10 00 10", 1, 10000)
         if r1 <> 0 then r1 = DL.WaitForSequence("df30", 1, 10000)
      else
         r1 = DL.WaitForSequence("60-10 00 10 (NEO2)", 1, 10000)
      end if

      if readerType <> "NEO2" then 
         r2 = DL.WaitForSequence("dfee26", 1, 2000)
      else
         r2 = DL.WaitForSequence("dfee26 (NEO2)", 1, 2000)
      end if

      if readerType <> "NEO2" then 
         r3 = DL.WaitForSequence("dfef4c-4d", 1, 2000)
      else
         r3 = 1
      end if

      if r1 = 0 or r2 = 0 or r3 = 0 then 
         result = 0
         dl.sendsequence "05-01 Cancel transaction"
         dl.pause 1000    
      end if     
   else
      dl.sendsequence "05-01 Cancel transaction"
      dl.pause 1000
   end if
end if

If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 60-11 Contact Authenticate Transaction"
   DL.SendSequence "60-11"
   result = DL.WaitForSequence("60-xx 63 status", 1, 10000)
   if result <> 0 then
      if readerType <> "NEO2" then 
         result = DL.WaitForSequence("60-11 00 04", 1, 10000)
         if result <> 0 then result = DL.WaitForSequence("df30 + dfee26", 1, 10000)
      else
         result = DL.WaitForSequence("60-11 00 04 (NEO2)", 1, 3000)
         if result = 0 then result = DL.WaitForSequence("60-11 00 04 (NEO2) 2", 1, 3000)
      end if
   else
      dl.sendsequence "05-01 Cancel transaction"
      dl.pause 1000
   end if

   if result <> 0 then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 60-13 Contact Retrieve Transaction Result"
      DL.SendSequence "60-13 (for 60-11)"
      if readerType <> "NEO2" then 
         result = DL.WaitForSequence("60-13 (for 60-11)", 1, 10000)
      else
         result = DL.WaitForSequence("60-13 (for 60-11) (NEO2)", 1, 10000)
      end if
      dl.pause 1000
   end if
end if

If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 60-12 Contact Apply Host Response"
   DL.SendSequence "60-12"
   result = DL.WaitForSequence("60-xx 63 status", 1, 10000)
   if result <> 0 then
      if readerType <> "NEO2" then 
         result = DL.WaitForSequence("60-12 00 02", 1, 10000)
         if result <> 0 then result = DL.WaitForSequence("9f5b + df30 + dfee26", 1, 10000)
      else
         result = DL.WaitForSequence("60-12 00 02 (NEO2)", 1, 10000)
      end if
   else
      dl.sendsequence "05-01 Cancel transaction"
      dl.pause 1000
   end if

   if result <> 0 then
      dl.pause 500
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 60-13 Contact Retrieve Transaction Result"
      DL.SendSequence "60-13 (for 60-12)"
      if readerType <> "NEO2" then 
         result = DL.WaitForSequence("60-13 (for 60-12)", 1, 10000)
      else
         result = DL.WaitForSequence("60-13 (for 60-12) (NEO2)", 1, 10000)
      end if

      dl.pause 1000
   end if
else
   dl.sendsequence "05-01 Cancel transaction"
   dl.pause 1000
end if

DL.AddComment vbCrLf
DL.AddComment "---------- END OF TEST ----------" & vbCrLf
PrintMyResult(result)

Else '""
  DL.AddComment vbCrLf & authnMsg & vbCrLf
End If '""
