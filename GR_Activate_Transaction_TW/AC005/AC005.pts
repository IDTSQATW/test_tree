   if readerType <> "NEO2" then r1 = DL.WaitForSequence("02 23", 1, 5000)
   if readerType = "NEO2" then r1 = DL.WaitForSequence("02 23 (NEO2)", 1, 5000)
   
   r2 = dl.waitforsequence("9f39", 1, 3000)
   
