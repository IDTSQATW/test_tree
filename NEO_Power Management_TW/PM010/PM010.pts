      if result2 = 0 then dl.addcomment vbcrlf & "9F39 >> ERROR!"
      if result3 = 0 then dl.addcomment vbcrlf & "FFEE01 >> ERROR!"
      if result4 = 0 then dl.addcomment vbcrlf & "DFEE26 >> ERROR!"
