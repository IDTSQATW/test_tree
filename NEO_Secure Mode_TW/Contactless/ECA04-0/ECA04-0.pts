'TEST BEGINS
'------------------------------------------------------------
fc = inputbox("The reader support:" & vbcrlf & vbcrlf & "1) 02-01 ONLY" & vbcrlf & "2) 02-40 ONLY" & vbcrlf & "3) 02-01/ 02-40 BOTH")
'------------------------------------------------------------encryption 00
if result <> 0 then
   dl.resetreceivecounter
   DL.AddComment "--- 04-F0"
   DL.SendSequence "04-F0"
   result = DL.WaitForSequence("burst off", 1, 2000)
   dl.pause 7000
else 
   countf = countf + 1
end if

if result <> 0 then
   precondition()
   if result <> 0 then
      loadkey_D()
   end if
else 
   countf = countf + 1
end if

if result <> 0 then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- Data Encryption: Enable 00"
   DL.SendSequence "C7-36 00"
   result = DL.WaitForSequence("enc on", 1, 2000)
   de = 0
else 
   countf = countf + 1
end if

If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- Burst mode Off"
   DL.SendSequence "burst mode off"
   result = DL.WaitForSequence("burst off", 1, 2000)
   dl.pause 500
else 
   countf = countf + 1
End If

if fc = 1 or fc = 3 then
   pollondemand()
   at0201()
   autopoll()
   at0201()
end if

if fc = 2 or fc = 3 then
   pollondemand()
   at0240()
   autopoll()
   at0240()
end if
'------------------------------------------------------------encryption 02/ 03
for k = 2 to 3
    DL.ClearCommWindows

    if result <> 0 then
       dl.resetreceivecounter
       dl.addcomment vbcrlf
       DL.AddComment "--- 04-F0"
       DL.SendSequence "04-F0"
       result = DL.WaitForSequence("burst off", 1, 2000)
       dl.pause 8000
    else 
       countf = countf + 1
    end if

    if result <> 0 then
       dl.resetreceivecounter
       dl.addcomment vbcrlf
       DL.AddComment "--- Data Encryption: Enable " & k
       DL.SendSequence "C7-36 0" & k
       result = DL.WaitForSequence("enc on", 1, 2000)
       de = k
    else 
       countf = countf + 1
    end if

    If result <> 0 Then
       dl.resetreceivecounter
       dl.addcomment vbcrlf
       DL.AddComment "--- Burst mode Off"
       DL.SendSequence "burst mode off"
       result = DL.WaitForSequence("burst off", 1, 2000)
       dl.pause 500
    else 
       countf = countf + 1
    End If

    if result <> 0 then
       precondition()
       if result <> 0 then
          loadkey_D()
       end if
    else 
       countf = countf + 1
    end if

    if result <> 0 then
       dl.resetreceivecounter
       dl.addcomment vbcrlf
       dl.addcomment "Erase All Keys..."
       dl.sendsequence "Erase All Keys"
       result = dl.waitforsequence("erase key ok", 1, 2000)
    else 
       countf = countf + 1
    end if

    if fc = 1 or fc = 3 then
       pollondemand()
       at0201()
       autopoll()
       at0201()
    end if
    if fc = 2 or fc = 3 then
       pollondemand()
       at0240()
       autopoll()
       at0240()
    end if
next
