sub eng()
    if result <> 0 then
       dl.resetreceivecounter
       dl.addcomment vbcrlf
       dl.addcomment "--- Language Option for LCD display: English"
       if readerType <> "NEO2" then 
          dl.sendsequence "language eng"
       else
          dl.sendsequence "language eng (NEO2)"
       end if
       result = dl.waitforsequence("lang ok resp", 1, 2000)
    end if
end sub

sub engfra()
    if result <> 0 then
       dl.resetreceivecounter
       dl.addcomment vbcrlf
       dl.addcomment "--- Language Option for LCD display: English + French"
       if readerType <> "NEO2" then 
          dl.sendsequence "language eng + fra"
       else
          dl.sendsequence "language eng + fra (NEO2)"
       end if
       result = dl.waitforsequence("lang ok resp", 1, 2000)
    end if
end sub
