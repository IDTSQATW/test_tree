    DL.Pause 1000

    DL.AddComment "*** Ping OPEN COM PORT ***"
    DL.SendSequence "Ping"
    result = DL.WaitForSequence ("Ping Successful", 1, 10000)
    DL.Pause 500
    DL.AddComment "*** Ping OPEN COM PORT *** - COMPLETE"  & vbCrLf
  Else
    errSts = errSts + 1
