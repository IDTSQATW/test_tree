          end if

          if k = 4 then 
             if readerType = "GR" or readerType = "NEO1" then
                result = DL.WaitForSequence("02-40 case 1", 1, 4000)
             elseif readerType = "NEO2" then
                result = DL.WaitForSequence("02-40 case 1 (NEO2)", 1, 4000)
             else
                result = DL.WaitForSequence("02-40 case 1", 1, 4000)
             end if
             ReverseResult(result)
