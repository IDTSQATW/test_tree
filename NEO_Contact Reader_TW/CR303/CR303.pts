sub CTFastEMVTran()
If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 60-10 Contact Start Transaction"
   DL.SendSequence ctcmd 
   result = DL.WaitForSequence("60-xx 63 status", 1, 10000)
   if result <> 0 then
      msgbox "Click OK, insert card (UL test card 12, Amex)", vbokonly

      if readerType = "GR" or readerType = "NEO1" then 
         result = DL.WaitForSequence("60-10 00 10", 1, 10000)
      elseif readerType = "NEO2" then 
         result = DL.WaitForSequence("60-12 00 03 FastEMV (NEO2)", 1, 10000)
      else
         r1 = DL.WaitForSequence("60-10 00 10", 1, 10000)
      end if
   end if

   if result <> 0 then
      beepcheck = msgbox ("The reader keep beeping?", vbyesno)
      if beepcheck = 7 then result = 0
      if beepcheck = 6 then 
         dl.resetreceivecounter
         dl.addcomment vbcrlf
         DL.AddComment "--- 03-03"
         dl.sendsequence "03-03"
         dl.pause 3000
      end if
      msgbox "Remove the card --> Click OK", vbokonly
      if result <> 0 then
         beepcheck = msgbox ("The reader stop beeping?", vbyesno)
         if beepcheck = 7 then result = 0
      end if
   end if
end if
end sub  

else
   DL.AddComment "The reader did not support FastEMV, please skip this case."
end if
