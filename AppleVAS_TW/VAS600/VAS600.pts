If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- Set Merchant 4 - Panera"
   DL.SendSequence "Set Merchant 4 - Panera"
   result = DL.WaitForSequence("04 00", 1, 2000)
   dl.pause 500
End If

for i = 1 to 3
    if i = 1 then 
       If result <> 0 Then
          dl.resetreceivecounter
          dl.addcomment vbcrlf & "======================================================="
          DL.AddComment "--- 01-0D - Auto output = Off"
          DL.SendSequence "01-0D - Auto output = Off"
          result = DL.WaitForSequence("01 00", 1, 2000)
          dl.pause 500
       End If
    end if

    if i = 2 then 
       If result <> 0 Then
          dl.resetreceivecounter
          dl.addcomment vbcrlf & "======================================================="
          DL.AddComment "--- 01-0D - Auto output = for good reads"
          DL.SendSequence "01-0D - Auto output = for good reads"
          result = DL.WaitForSequence("01 00", 1, 2000)
          dl.pause 500
       End If
    end if

    if i = 3 then 
       If result <> 0 Then
          dl.resetreceivecounter
          dl.addcomment vbcrlf & "======================================================="
          DL.AddComment "--- 01-0D - Auto output = for good & bad reads"
          DL.SendSequence "01-0D - Auto output = for good & bad reads"
          result = DL.WaitForSequence("01 00", 1, 2000)
          dl.pause 500
       End If
    end if
    '------------------------------------------------------------4 terminal type
    for j = 1 to 4
        if j = 1 then 
           if result <> 0 then
              dl.resetreceivecounter
              dl.addcomment vbcrlf
              DL.AddComment "--- 04-00 w/ tag FFEE06 (VAS only)"
              DL.SendSequence "04-00 w/ tag FFEE06 (VAS only)"
              result = DL.WaitForSequence("04 00", 1, 2000)
              dl.pause 500
           end if
        end if

        if j = 2 then 
           if result <> 0 then
              dl.resetreceivecounter
              dl.addcomment vbcrlf
