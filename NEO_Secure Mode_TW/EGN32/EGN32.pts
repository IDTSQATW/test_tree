Sub PrintMyResult(ResultX)
      If ResultX <> 0 And errCnt = 0 And caseValid <> 0 Then
            MasterResult = 1
            DL.AddComment "Result = PASSED"
      Else
            MasterResult = 0
            DL.AddComment "Result = FAILED"
      End If
End Sub


Function ReverseResult(ResultParam)
      If ResultParam = 1 Then
            result = 0
      ElseIf ResultParam = 0 Then
            result = 1
      End If
End Function

sub countfail()
    for i = 0 to 5
        if r(i) = 0 then
           countf = countf + 1
        end if
    next
end sub

sub enctype()
for k = 1 to 2
If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 0-use TDES to encrypt (C7-32)"
   DL.SendSequence "0-use TDES to encrypt (C7-32)"
   result = DL.WaitForSequence("C7-xx ok resp", 1, 5000)
   dl.pause 300
End If
If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- C7-33"
   DL.SendSequence "C7-33"
   result = DL.WaitForSequence("C733 00", 1, 5000)
   dl.pause 300
End If

If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- 1-use AES to encrypt (C7-32)"
   DL.SendSequence "1-use AES to encrypt (C7-32)"
   result = DL.WaitForSequence("C7-xx ok resp", 1, 5000)
   dl.pause 300
End If
If result <> 0 Then
   dl.resetreceivecounter
   dl.addcomment vbcrlf
   DL.AddComment "--- C7-33"
   DL.SendSequence "C7-33"
   result = DL.WaitForSequence("C733 01", 1, 5000)
   dl.pause 300
End If

if transarmor = 6 then
   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- 2-use TransArmor TDES to encrypt (C7-32)"
      DL.SendSequence "2-use TransArmor TDES to encrypt (C7-32)"
      result = DL.WaitForSequence("C7-xx ok resp", 1, 5000)
      dl.pause 300
   End If
   If result <> 0 Then
      dl.resetreceivecounter
      dl.addcomment vbcrlf
      DL.AddComment "--- C7-33"
      DL.SendSequence "C7-33"
      result = DL.WaitForSequence("C733 02", 1, 5000)
      dl.pause 300
   End If
end if
next
end sub
